    "@effect/vitest": "catalog:",
    "@types/node": "24.3.0",
    "dotenv-cli": "10.0.0",
    "eslint": "catalog:",
    "eslint-plugin-local-rules": "3.0.2",
    "prettier": "catalog:",
    "tsup": "catalog:",
